  <!-- 根据Map中的条件查询销售指标 -->
	<select id="getListMap" parameterType="map" resultType="map">
		SELECT
			st.*, u.username,
			u.nickname
		FROM
			sales_target st
		LEFT JOIN `user` u ON st.user_id = u.id
		WHERE
			u.deleted = 1
		AND u.type = 1
	</select>
