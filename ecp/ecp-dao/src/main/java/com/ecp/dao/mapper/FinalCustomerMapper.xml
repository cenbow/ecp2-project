  <!-- 根据最终用户名称查询 -->
  <select id="getFinalCustomerByOrganizationName" resultType="map">
  	 SELECT f.* 
  	 	FROM final_customer f  	 		 
  	 	<where>  	 		 
  	 		f.deleted=1
  	 	<!-- 组织名称条件 -->	
  	 	<if test="organizationName!=null and organizationName!='' ">
  			AND f.organization_name LIKE concat(concat('%',#{organizationName}),'%')	
  		</if>
  	 </where>  	 
  	 	  	 
  </select>
  
  
  
