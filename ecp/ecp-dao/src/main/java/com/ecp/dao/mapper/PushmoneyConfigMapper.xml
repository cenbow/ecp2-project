  <select id="getList" resultType="map">
  	SELECT
		c.*, r.role_code, r.role_name
	FROM
		pushmoney_config c
	LEFT JOIN role r ON c.role_id = r.role_id
	WHERE
		r.deleted = 1
		<if test="role_id_list!=null and role_id_list.size()>0">
			and c.role_id IN 
